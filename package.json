{
  "name": "nodejs-sample",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "jest --collectCoverage",
    "prebuild": "rimraf dist/",
    "build": "tsc --project tsconfig.json",
    "start": "npm run serve",
    "serve": "node dist/index.js",
    "start:dev": "nodemon",
    "watch-ts": "tsc -w",
    "typeorm": "tsx ./node_modules/typeorm/cli",
    "seeding": "tsx ./node_modules/@jorgebodega/typeorm-seeding/dist/cli",
    "migration:generate": "npm run typeorm -- -d ./src/db/typeorm.config.ts migration:generate ./src/db/migrations/$npm_config_name",
    "migration:run": "npm run typeorm -- -d ./src/db/typeorm.config.ts migration:run",
    "migration:revert": "npm run typeorm -- -d ./src/db/typeorm.config.ts migration:revert",
    "seed:run": "npm run seeding -- seed -d ./src/db/typeorm.config.ts ./src/db/seeds/*.ts"
  },
  "devDependencies": {
    "@babel/core": "^7.24.3",
    "@jorgebodega/typeorm-seeding": "^7.0.0",
    "@types/bcrypt": "^5.0.2",
    "@types/body-parser": "^1.19.5",
    "@types/cors": "^2.8.17",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.21",
    "@types/jest": "^26.0.24",
    "@types/jsdom": "^21.1.6",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/lodash": "^4.17.0",
    "@types/mailgun-js": "^0.22.18",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.11",
    "@types/multer-s3": "^2.7.12",
    "@types/node": "^20.11.30",
    "@types/supertest": "^2.0.16",
    "@types/swagger-ui-express": "^4.1.6",
    "@types/uuid": "^8.3.4",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-simple-import-sort": "^12.0.0",
    "eslint-plugin-unused-imports": "^3.1.0",
    "jest": "^26.6.3",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.8",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.5.6",
    "tsx": "^4.7.1",
    "typescript": "^4.9.5",
    "typescript-eslint": "^7.3.1"
  },
  "dependencies": {
    "@whiskeysockets/baileys": "^6.6.0",
    "aws-sdk": "^2.1581.0",
    "axios": "^1.6.8",
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "class-validator-jsonschema": "^5.0.0",
    "cors": "^2.8.5",
    "dotenv": "^6.2.0",
    "express": "^4.19.0",
    "express-joi-validator": "^2.0.1",
    "express-jwt": "^6.1.2",
    "express-winston": "^3.4.0",
    "google-auth-library": "^9.7.0",
    "himalaya": "^1.1.0",
    "http-status-codes": "^1.4.0",
    "jsdom": "^24.0.0",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "lodash.capitalize": "^4.2.1",
    "lodash.merge": "^4.6.2",
